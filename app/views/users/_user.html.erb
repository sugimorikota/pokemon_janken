<div id="<%= dom_id user %>">
  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Crypted password:</strong>
    <%= user.crypted_password %>
  </p>

  <p>
    <strong>Salt:</strong>
    <%= user.salt %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

</div>
